# MeghSutra AI - Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Google Gemini API (for AI responses - FREE tier available)
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Ollama Configuration (optional - for local AI)
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=phi3

# Twilio Configuration (for WhatsApp Bot)
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# Application Settings
APP_NAME=MeghSutra AI
APP_VERSION=1.0.0
DEBUG_MODE=False
